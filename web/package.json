{
  "name": "img2pic_web",
  "version": "0.0.1",
  "description": "将AI生成的伪像素画转换成真像素画",
  "productName": "img2pic_web",
  "author": "AndreaFrederica <andreafrederica@outlook.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:pwa": "cross-env DEV_PWA=true quasar dev -m pwa",
    "build": "quasar build -m pwa",
    "build:pwa": "quasar build -m pwa",
    "build:spa": "quasar build -m spa",
    "build:cf": "quasar build -m pwa",
    "build:wasm": "node scripts/build-wasm.js",
    "deploy:cf": "npm run build:cf && cd workers-site && npm install && npm run deploy:production",
    "deploy:cf:staging": "npm run build:cf && cd workers-site && npm install && npm run deploy:staging",
    "generate-icons": "node scripts/generate-icons.js",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "comlink": "^4.4.2",
    "file-saver": "^2.0.5",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "upng-js": "^2.1.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/node": "^20.5.9",
    "@types/pngjs": "^6.0.5",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "sharp": "^0.34.5",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7",
    "workbox-build": "^7.4.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
